<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>addSomeServlet</servlet-name>
        <servlet-class>servlets.addSomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addSomeServlet</servlet-name>
        <url-pattern>/addSomeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delSomeServlet</servlet-name>
        <servlet-class>servlets.delSomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delSomeServlet</servlet-name>
        <url-pattern>/delSomeServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delClick</servlet-name>
        <servlet-class>servlets.delClick</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delClick</servlet-name>
        <url-pattern>/delClick</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchServlet</servlet-name>
        <servlet-class>servlets.searchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchServlet</servlet-name>
        <url-pattern>/searchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showAllServlet</servlet-name>
        <servlet-class>servlets.showAllServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showAllServlet</servlet-name>
        <url-pattern>/showAllServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>wayServlet</servlet-name>
        <servlet-class>servlets.wayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wayServlet</servlet-name>
        <url-pattern>/wayServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateSomeServlet</servlet-name>
        <servlet-class>servlets.updateSomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateSomeServlet</servlet-name>
        <url-pattern>/updateSomeServlet</url-pattern>
    </servlet-mapping>


</web-app>