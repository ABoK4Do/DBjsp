<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>addOneAJAX</servlet-name>
        <servlet-class>servlets.addOneAJAX</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addOneAJAX</servlet-name>
        <url-pattern>/addOneAJAX</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delOneAJAX</servlet-name>
        <servlet-class>servlets.delOneAJAX</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delOneAJAX</servlet-name>
        <url-pattern>/delOneAJAX</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>updateOneAJAX</servlet-name>
        <servlet-class>servlets.updateOneAJAX</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateOneAJAX</servlet-name>
        <url-pattern>/updateOneAJAX</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showAllAJAX</servlet-name>
        <servlet-class>servlets.showAllAJAX</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showAllAJAX</servlet-name>
        <url-pattern>/showAllAJAX</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchAJAX</servlet-name>
        <servlet-class>servlets.searchAJAX</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchAJAX</servlet-name>
        <url-pattern>/searchAJAX</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addSomeAJAX</servlet-name>
        <servlet-class>servlets.addSomeAJAX</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addSomeAJAX</servlet-name>
        <url-pattern>/addSomeAJAX</url-pattern>
    </servlet-mapping>


</web-app>