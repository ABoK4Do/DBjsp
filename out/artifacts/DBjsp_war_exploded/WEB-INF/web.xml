<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>addOneServlet</servlet-name>
        <servlet-class>servlets.addOneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addOneServlet</servlet-name>
        <url-pattern>/addOneServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delOneServlet</servlet-name>
        <servlet-class>servlets.delOneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delOneServlet</servlet-name>
        <url-pattern>/delOneServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delClick</servlet-name>
        <servlet-class>servlets.delClick</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delClick</servlet-name>
        <url-pattern>/delClick</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchServlet</servlet-name>
        <servlet-class>servlets.searchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchServlet</servlet-name>
        <url-pattern>/searchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>showAllServlet</servlet-name>
        <servlet-class>servlets.showAllServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showAllServlet</servlet-name>
        <url-pattern>/showAllServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>wayServlet</servlet-name>
        <servlet-class>servlets.wayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wayServlet</servlet-name>
        <url-pattern>/wayServlet</url-pattern>
    </servlet-mapping>
</web-app>